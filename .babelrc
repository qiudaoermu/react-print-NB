{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "debug": false,
        "modules": false,
        "targets": "> 0.5%, last 2 versions, Firefox ESR, iOS >= 8, Android >= 4, ie >= 10, not dead"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-class-properties",
    "babel-plugin-transform-react-remove-prop-types"
  ]
}
